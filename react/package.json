{
  "name": "circuitsvis",
  "description": "Mechanistic Interpretability Visualizations",
  "version": "1.18.0",
  "main": "dist/commonjs/index.js",
  "module": "dist/module/index.js",
  "unpkg": "dist/cdn/iife.js",
  "jsdelivr": "dist/cdn/iife.js",
  "source": "src/index.ts",
  "types": "dist/module/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.mjs",
      "require": "./dist/index.cjs.js"
    }
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint",
    "buildBrowser": "node ./esbuild.js",
    "buildNode": "tsc --project tsconfig.build.json",
    "build": "yarn buildBrowser && yarn buildNode",
    "typeCheck": "tsc --noEmit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": "https://github.com/alan-cooney/CircuitsVis",
  "author": "Alan Cooney",
  "license": "MIT",
  "dependencies": {
    "@tensorflow/tfjs": "^4.0.0",
    "chart.js": "^4.0.1",
    "chartjs-chart-matrix": "^1.3.0",
    "colord": "^2.9.3",
    "react-chartjs-2": "^5.0.1",
    "react-grid-system": "^8.1.6",
    "react-popper-tooltip": "^4.4.2",
    "tinycolor2": "^1.4.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@tensorflow/tfjs-node": "^4.0.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.2.2",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/tinycolor2": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "esbuild": "^0.15.13",
    "esbuild-plugin-external-global": "^1.0.1",
    "eslint": "8.27.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.4",
    "eslint-plugin-jest-dom": "4.0.2",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "^0.6.7",
    "eslint-plugin-testing-library": "5.9.1",
    "jest": "^29.2.2",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "29.2.2",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "testing-library": "^0.0.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17 || ^18"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest"
      ]
    },
    "setupFiles": [
      "jest-canvas-mock"
    ]
  }
}
